<#include "../public_header.ftl">


	<script type="text/javascript" src="/static/js/varietyManagement/edit.js"></script>

</head>
<body>
<div class="zq-wrap">
	<div class="layui-card bg-1">
		<div class="layui-card-body">
			<form action="" class="layui-form layui-form-pane zq-form" lay-filter="formTest">
				<div class="zq-btn-list">
					<button class="layui-btn green" lay-submit lay-filter="*">提交</button>
					<button class="layui-btn blue zq-active" type="button" data-type="zq_closeIframe">关闭</button>

				</div>
				<div class="layui-row zq-form-wrap zq-margin-t30">
					<div class="layui-form-item">
						<label class="layui-form-label">
							品种值
						</label>
						<div class="layui-input-block">
							<input type="text" name="varietyValue" readonly value="${varietyValue}" lay-verify="required" class="layui-input"/>
						</div>
					</div>
					<div class="layui-form-item">
						<label class="layui-form-label">
							中文名称
						</label>
						<div class="layui-input-block">
							<input type="text" name="cnName" lay-verify="required" class="layui-input"/>
						</div>
					</div>
					<div class="layui-form-item">
						<label class="layui-form-label">
							英文名称
						</label>
						<div class="layui-input-block">
							<input type="text" name="elName" class="layui-input"/>
						</div>
					</div>
					<div class="layui-form-item">
						<label class="layui-form-label">
							日文名称
						</label>
						<div class="layui-input-block">
							<input type="text" name="jpName" class="layui-input"/>
						</div>
					</div>


				</div>
			</form>
		</div>
	</div>
</div>





</body>
</html>