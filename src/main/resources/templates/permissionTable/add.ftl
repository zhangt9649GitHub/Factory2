<#include "../public_header.ftl">



	<script type="text/javascript" src="/static/js/permissionTable/add.js"></script>

</head>
<body>

<div class="zq-wrap">
	<div class="layui-card bg-1">
		<div class="layui-card-body">
			<form action="" class="layui-form layui-form-pane zq-form">
				<div class="zq-btn-list">
					<button class="layui-btn green" lay-submit lay-filter="*">提交</button>
					<button class="layui-btn blue zq-active" type="button" data-type="zq_closeIframe">关闭</button>

				</div>
				<div class="layui-row zq-form-wrap zq-margin-t30">
					<div class="layui-form-item">
						<label class="layui-form-label">
							名称
						</label>
						<div class="layui-input-block">
							<input type="text" name="accessName" lay-verify="required" autocomplete="off" class="layui-input"/>
						</div>
					</div>
					<div class="layui-form-item">
						<label class="layui-form-label">
							值
						</label>
						<div class="layui-input-block">
							<input type="text" name="permissions" lay-verify="required" autocomplete="off" class="layui-input"/>
						</div>
					</div>
					<div class="layui-form-item">
						<label class="layui-form-label">
							接口
						</label>
						<div class="layui-input-block">
							<input type="text" name="url" autocomplete="off" class="layui-input"/>
						</div>
					</div>

					<div class="layui-form-item">
						<label class="layui-form-label">
							父Id
						</label>
						<div class="layui-input-block">
							<input type="text" name="accessModuleId" lay-verify="required|number" value="${accessModuleId}" autocomplete="off" class="layui-input"/>
						</div>
					</div>

				</div>
			</form>
		</div>
	</div>
</div>





</body>
</html>