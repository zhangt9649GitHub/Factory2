    padding: 0px;
}

.zq-wrap .layui-col-space15 .layui-card-header{
    padding: 0 15px;
    /*color: #FFFF;*/
    font-weight: 600;
    font-family: "Karla", sans-serif;
}
.zq-wrap .layui-col-space15 .layui-card-body{
    padding: 10px 15px;
}

