



.zq-wrap{
    min-height: 100%;
    background-color: transparent;
    position: relative;
    padding: 20px;
    padding-bottom: 50px;
}
.card-list{
    height: 150px;
    line-height: 30px;
}
.layui-card-body.card-list .item,
.layui-card-body.card-list .item a{
    color: rgba(255,255,255,0.9);
}
.layui-card-body.card-list .item:hover,
.layui-card-body.card-list .item a:hover{
    color: rgba(255,255,255,1);
}
.layui-card-body.card-list .layui-btn.white{
    display: inline-block;
    height: 38px;
    line-height: 38px;
    padding: 0 18px;
    color: #fff;
    white-space: nowrap;
    text-align: center;
    font-size: 14px;
    border-radius: 4px;
    cursor: pointer;
    margin-right: 5px;
    background-color: transparent;
    border: 2px solid #fff;
    font-weight: 700;
}
.layui-card-body.card-list .layui-btn.white .layui-icon {
    margin-right: 3px;
    font-size: 14px;
    vertical-align: top;
}
.cord-badge{
    position: absolute;
    right: 15px;
    top: 50%;
    margin-top: -9px;
}

