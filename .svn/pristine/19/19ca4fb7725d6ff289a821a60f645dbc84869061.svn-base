.echarts{
    height: 300px;
    width: 100%;
}



.table2 .wrap{
    /*position: relative;*/
    width: 100%;
    box-sizing: border-box;
    border-width: 1px;
    border-style: solid;
    border-color: #e6e6e6;
    margin-top: -11px;
    background-color: #f2f2f2;
    overflow-x: hidden;
}
.table2 .wrap .list{
    /*position: absolute;*/
    display: flex;
    text-align: center;
}
.table2 .wrap .list .item{
    flex: 1;
    border-width: 0 1px 0 0;
    border-style: solid;
    border-color: #e6e6e6;
    padding: 5px 0;
    box-sizing: border-box;
}
.table2 .wrap .list .item .cell{
    padding: 0 15px;
    height: 28px;
    line-height: 28px;
    box-sizing: border-box;
}
.table2 .wrap .list .item:nth-child(2),
.table2 .wrap .list .item:nth-child(8),
.table2 .wrap .list .item:last-child{
    width: 151px;
    flex: unset;
}
