<#include "../public_header.ftl">


	<link rel="stylesheet" type="text/css" href="/static/css/pickingList/open.css">
	<script type="text/javascript" src="/static/js/pickingList/open.js"></script>

</head>
<body>

<div class="zq-wrap">

	<div class="layui-row">

		<div class="layui-card bg-1 layui-form" lay-filter="formTest">

			<div class="layui-card-header layui-clear">
				<h3 class="left">查看-领料单 <span></span></h3>
				<div class="zq-btn-list">
				</div>
			</div>
			<div class="layui-card-body">

				<div class="zq-container zq-min-h800">

					<form>
						<input type="hidden" name="pickingId" value="${pickingId}">
						<div class="layui-row">
							<div class="layui-col-md6">
								<div class="zq-item">
									<label>编号：</label>
									<input type="text" name="pickingNumber" class="layui-input" readonly value="1929831239" >
								</div>
							</div>
							<div class="layui-col-md6 zq-text-align-r">
								<div class="zq-item">
									<label>日期：</label>
									<input type="text" name="addTime" class="layui-input" autocomplete="off" readonly value="2019-02-19">
								</div>
							</div>

							<div class="layui-col-md4">
								<div class="zq-item">
									<label>批号：</label>
									<input type="text" name="lotNumber" class="layui-input" readonly value="1929831239">
								</div>
							</div>
							<div class="layui-col-md4">
								<div class="zq-item">
									<label>领出仓库：</label>
									<input type="text" name="outWarehouseId" readonly value="仓库001">
								</div>
							</div>
							<div class="layui-col-md4 zq-text-align-r">
								<div class="zq-item">
									<label>领入仓库：</label>
									<input type="text" name="intoWarehouseId" readonly value="仓库002">
								</div>
							</div>


							<div class="zq-datatable">
								<table id="idTest" lay-filter="demo" class="layui-table"></table>
							</div>
							<!-- 备注 -->

							<div class="remarks">
								<textarea name="comment" class="layui-textarea" readonly></textarea>
							</div>

							<div class="layui-col-md6">
								<div class="zq-item">
									<label>创建人：</label>
									<input type="text" name="staffName" readonly >
								</div>
							</div>


						</div>

					</form>


				</div>



			</div>

		</div>

	</div>



</div>




</body>
</html>