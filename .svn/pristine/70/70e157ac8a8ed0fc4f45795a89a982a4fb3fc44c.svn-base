

#tree-table tbody tr td:first-child{
    cursor: pointer;
}
#tree-table{
    width: 100%;
    min-width: 3000px;
    margin: 0;
}
#tree-table thead tr th{
    border-top: 0;
}
#tree-table thead tr th:first-child,
#tree-table tbody tr td:first-child{
    border-left: 0;
}
#tree-table thead tr th:last-child,
#tree-table tbody tr td:last-child{
    border-right: 0;
}
.wrap{
    height: 440px;
    border: 1px solid #e6e6e6;
    margin-top: 30px;
    /*overflow: auto;*/
}

.layui-laypage a,
.layui-laypage span{
    margin-bottom: 0;
    height: 26px;
    line-height: 26px;
    border: 0;
    padding: 0 10px;
}
