/* 操作 */

.shrink,
.left-page,
.right-page{
	position: absolute;
	width: 40px;
	height: 40px;
	line-height: 40px;
	text-align: center;
	box-sizing: border-box;
	cursor: pointer;
	z-index: 1;
	/*border-bottom: 1px solid #e6e6e6;*/
	/*border-right: 1px solid #e6e6e6;*/
	background-color: #fff;
	color: #6f6f6f;
}
.left-page{
	margin-left: 40px;
}
.right-page {
	right: 40px;

}
.right-page:hover,
.left-page:hover,
.shrink:hover{
	color: #00a2d9;
}

.operation {
	position: absolute;
	width: 40px;
	height: 40px;
	top: 0;
	line-height: 40px;
	right: 0;
	text-align: center;
	box-sizing: border-box;
	cursor: pointer;
	z-index: 1;
	/*border-bottom: 1px solid #e6e6e6;*/
	background-color: #ffff;
	color: #6f6f6f;
}

.operation:hover{
	color: #00a2d9;
}

.operation .layui-nav {
	position: absolute;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
	padding: 0;
	background-color: transparent;
}

.operation .layui-nav .layui-nav-item {
	height: 40px;
}

.operation .layui-nav-bar {
	width: 0 !important;

}

.operation .layui-nav .layui-nav-child {
	right: 0;
	left: unset;
	top: 40px;
}
.operation .layui-nav .layui-nav-child dd.layui-this,
.operation .layui-nav .layui-nav-child dd.layui-this a{
	background-color: #417ba2;
}

/* 操作 */
