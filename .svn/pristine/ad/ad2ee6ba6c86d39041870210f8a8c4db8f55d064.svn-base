<#include "../public_header.ftl">



<script type="text/javascript" src="/static/js/receipt/open.js"></script>

</head>
<body>

<div class="zq-wrap">

	<div class="layui-row">

		<div class="layui-card bg-1 layui-form" lay-filter="formTest">

			<div class="layui-card-header layui-clear">
				<h3 class="left">查看-收款单 <span></span></h3>
				<div class="zq-btn-list">
				</div>
			</div>
			<div class="layui-card-body">

				<div class="zq-container zq-min-h800">

					<form action="">
						<input type="hidden" name="receiptId" value="${receiptId}">
						<div class="layui-row">
							<div class="layui-col-md6">
								<div class="zq-item">
									<label>编号：</label>
									<input type="text" name="receiptNumber" readonly class="layui-input">
								</div>
							</div>
							<div class="layui-col-md6 zq-text-align-r">
								<div class="zq-item">
									<label>日期：</label>
									<input type="text" name="cjTime" readonly class="layui-input">
								</div>
							</div>
							<table class="zq-table">

								<tbody>
								<tr>
									<th >
										<div>
											收款类型
										</div>
									</th>
									<td >
										<div>
											<input type="text" name="category" readonly class="layui-input">

										</div>
									</td>
									<th >
										<div>
											付款方
										</div>
									</th>
									<td >
										<div >
											<input type="text" name="payer" readonly/>
										</div>
									</td>
									<th >
										<div>
											关联单据
										</div>
									</th>
									<td >
										<div>
											<input type="text" name="associatedDocument" readonly/>
										</div>

									</td>
								</tr>

								<tr>
									<th >
										<div>
											单据金额
										</div>
									</th>
									<td >
										<div>
											<input type="text" name="amount" readonly/>
										</div>
									</td>
									<th >
										<div>
											应收金额
										</div>
									</th>
									<td >
										<div>
											<input type="text" name="amountReceivable" readonly/>
										</div>
									</td>
									<th >
										<div>
											待核销金额
										</div>
									</th>
									<td >
										<div>
											<input type="text" name="pendingWriteOffAmount" readonly/>
										</div>
									</td>
								</tr>

								<tr>
									<th >
										<div>
											收款金额
										</div>
									</th>
									<td >
										<div>
											<input type="text" name="collectionAmount" readonly/>
										</div>
									</td>
									<th >
										<div>
											核销金额
										</div>
									</th>
									<td >
										<div>
											<input type="text" name="writeOffAmount" readonly/>
										</div>
									</td>
									<th >
										<div>
											支付方式
										</div>
									</th>
									<td >
										<div>
											<input type="text" name="paymentMethod" readonly/>
										</div>
									</td>
								</tr>



								</tbody>
							</table>


							<!-- 备注 -->

							<div class="remarks">
								<textarea name="comment" readonly class="layui-textarea"></textarea>
							</div>

							<div class="layui-col-md6">
								<div class="zq-item">
									<label>创建人：</label>
									<input type="text" name="name" readonly/>
								</div>
							</div>


						</div>

					</form>


				</div>



			</div>

		</div>

	</div>



</div>





</body>
</html>